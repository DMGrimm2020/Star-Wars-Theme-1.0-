<?xml version="1.0" encoding="utf-8" ?>

<Defs>

    <!--================================ Sculptures ======================================-->

    <ThingDef ParentName="SculptureBase">
        <defName>TGMaleSculpture</defName>
        <label>Male Sculpture</label>
        <description>A torso-sized piece of material sculpted into an artistic form.</description>
        <graphicData>
            <texPath>Things/Building/Furniture/Art/TGMaleSculpture</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <shaderType>CutoutComplex</shaderType>
        </graphicData>
        <fillPercent>0.35</fillPercent>
        <rotatable>false</rotatable>
        <statBases>
            <MaxHitPoints>90</MaxHitPoints>
            <Mass>3</Mass>
            <Beauty>50</Beauty>
            <WorkToMake>18000</WorkToMake>
        </statBases>
        <costStuffCount>50</costStuffCount>
        <stuffCategories>
            <li>Stony</li>
        </stuffCategories>
    </ThingDef>

    <ThingDef ParentName="SculptureBase">
        <defName>TGFemaleSculpture</defName>
        <label>Female Sculpture</label>
        <description>A torso-sized piece of material sculpted into an artistic form.</description>
        <graphicData>
            <graphicClass>Graphic_Single</graphicClass>
            <texPath>Things/Building/Furniture/Art/TGFemaleSculpture</texPath>
        </graphicData>
        <fillPercent>0.35</fillPercent>
        <rotatable>false</rotatable>
        <statBases>
            <MaxHitPoints>90</MaxHitPoints>
            <Mass>3</Mass>
            <Beauty>50</Beauty>
            <WorkToMake>18000</WorkToMake>
        </statBases>
        <costStuffCount>50</costStuffCount>
        <stuffCategories>
            <li>Stony</li>
        </stuffCategories>
    </ThingDef>

</Defs>